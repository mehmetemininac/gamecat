<h1>Listing contents</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Gametype</th>
    <th>Content data</th>
    <th>Contenttype</th>
    <th>Comment</th>
  </tr>

<% @contents.each do |content| %>
  <tr>
    <td><%=h content.title %></td>
    <td><%=h content.gametype_id %></td>
    <td><%=h content.content_data %></td>
    <td><%=h content.contenttype_id %></td>
    <td><%=h content.comment %></td>
    <td><%= link_to 'Show', content %></td>
    <td><%= link_to 'Edit', edit_content_path(content) %></td>
    <td><%= link_to 'Destroy', content, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New content', new_content_path %>