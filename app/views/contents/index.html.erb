<% title "Contents" %>

<table>
  <tr>
    <th>Name</th>
    <th>Contenttype</th>
    <th>Gamegenere</th>
    <th>Contentcategory</th>
    <th>Disc</th>
    <th>Cracked</th>
    <th>Original</th>
    <th>Moviegenere</th>
    <th>Moviequality</th>
    <th>Movieyear</th>
    <th>Subtitules</th>
    <th>Comment</th>
  </tr>
  <% for content in @contents %>
    <tr>
      <td><%=h content.name %></td>
      <td><%=h content.contenttype_id %></td>
      <td><%=h content.gamegenere_id %></td>
      <td><%=h content.contentcategory_id %></td>
      <td><%=h content.disc_id %></td>
      <td><%=h content.cracked %></td>
      <td><%=h content.original %></td>
      <td><%=h content.moviegenere_id %></td>
      <td><%=h content.moviequality %></td>
      <td><%=h content.movieyear %></td>
      <td><%=h content.subtitules %></td>
      <td><%=h content.comment %></td>
      <td><%= link_to "Show", content %></td>
      <td><%= link_to "Edit", edit_content_path(content) %></td>
      <td><%= link_to "Destroy", content, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Content", new_content_path %></p>
