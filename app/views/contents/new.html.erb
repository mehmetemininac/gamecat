<h1>New content</h1>

<% form_for(@content) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :gametype_id %><br />
    <%= f.text_field :gametype_id %>
  </p>
  <p>
    <%= f.label :content_data %><br />
    <%= f.text_area :content_data %>
  </p>
  <p>
    <%= f.label :contenttype_id %><br />
    <%= f.text_field :contenttype_id %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_area :comment %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contents_path %>