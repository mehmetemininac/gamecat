<% title "Contentcategories" %>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>
  <% for contentcategory in @contentcategories %>
    <tr>
      <td><%=h contentcategory.name %></td>
      <td><%=h contentcategory.description %></td>
      <%= common_index_links contentcategory %>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Contentcategory", new_contentcategory_path %></p>
<p><%= link_to "Home", root_path %></p>