<% title "Containers" %>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>
  <% for container in @containers %>
    <tr>
      <td><%=h container.name %></td>
      <td><%=h container.description %></td>
      <%= common_links :list, container %>
    </tr>
  <% end %>
</table>
<%= common_links :main %>
