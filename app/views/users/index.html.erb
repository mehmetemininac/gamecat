<% title "Users" %>

<table>
  <tr>
    <th>Username</th>
    <th>Role</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%=h user.username %></td>
      <td><%=h "LOL"  %></td>
      <%= common_links :section => :list, :object => user %>
    </tr>
  <% end %>
</table>
<%= common_links :section => :main, :klass => User %>
