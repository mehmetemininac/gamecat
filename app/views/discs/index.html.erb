<% title "Discs" %>

<table>
  <tr>
    <th>Number</th>
    <th>Label</th>
    <th>Disktype</th>
    <th>Given</th>
    <th>Given To</th>
    <th>Gived On</th>
    <th>Container</th>
    <th>Comment</th>
  </tr>
  <% for disc in @discs %>
    <tr>
      <td><%=h disc.number %></td>
      <td><%=h disc.label %></td>
      <td><%=h disc.disktype_id %></td>
      <td><%=h disc.given %></td>
      <td><%=h disc.given_to %></td>
      <td><%=h disc.gived_on %></td>
      <td><%=h disc.container_id %></td>
      <td><%=h disc.comment %></td>
      <td><%= link_to "Show", disc %></td>
      <td><%= link_to "Edit", edit_disc_path(disc) %></td>
      <td><%= link_to "Destroy", disc, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Disc", new_disc_path %></p>
